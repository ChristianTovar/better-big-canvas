<section>
  <div class="board">
    <%= for i <- row_enumerable() do %>
      <div class="row">
        <%= for j <- 0..19 do %>
          <div class="column board-square">
            <%= live_component @socket, BetterBigCanvasWeb.BoardComponent, id: i + j %>
          </div>
        <% end %>
      </div>
    <% end %>
  </div>
</section>
